<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingStart="25dp"
    android:paddingEnd="25dp"
    android:orientation="vertical">

    <ListView
        android:id="@+id/list_vehicle_restore"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@android:color/transparent"
        tools:listitem="@layout/list_item_vehicle_restore">
    </ListView>

    <TextView
        android:id="@+id/restore_dialog_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/colorWarningRed"
        android:text="@string/googleDrive_import_dialog_warning"/>


</LinearLayout>